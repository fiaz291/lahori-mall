{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "buildCommand": "npm install && npm run build && npx prisma generate && npx prisma migrate dev"
  }
  